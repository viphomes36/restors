$(function(){
	$('.slick-client').slick({
		dots: true,
			autoplay: false,
			draggable: false,
			infinite: false,
			slidesToShow: 3,
			slidesToScroll: 3,
			dotsClass: 'new-dots',
			prevArrow: '<button type="button" class="new-but-c slick-cl-pr"><i class="fas fa-angle-left"></i></button>',
			nextArrow: '<button type="button" class="new-but-c slick-cl-nt"><i class="fas fa-angle-right"></i></button>',
			rows: 2,
			responsive: [{
				breakpoint: 990,
				settings: {
					draggable: false,
					dots: true,
					slidesToScroll: 2,
					slidesToShow: 2,
					rows: 2,
				}
			}, {
				breakpoint: 790,
				settings: {
					draggable: false,
					dots: false,
					slidesToScroll: 1,
					slidesToShow: 1,
					rows: 2,
				}
			},{
				breakpoint: 390,
				settings: {
					draggable: false,
					dots: false,
					slidesToScroll: 1,
					slidesToShow: 1,
					rows: 1,
				}
			}]
	})
})

