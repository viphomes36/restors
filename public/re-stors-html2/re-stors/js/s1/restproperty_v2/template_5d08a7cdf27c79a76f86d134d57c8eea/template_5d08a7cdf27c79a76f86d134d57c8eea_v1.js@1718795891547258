	$('.input-page').on('change', function(){
		var quantPage = $('.page-text').text();
		var maxPage = parseInt(quantPage.replace(/\D+/g,""));
		var useQuant = $(this).val();
		if(useQuant > maxPage){
			$(this).val(maxPage);
		}else if(useQuant < 1){
			$(this).val('1');
		}
	})

	$('.switch-quant__button').on('click', '.quant__button', function(){
