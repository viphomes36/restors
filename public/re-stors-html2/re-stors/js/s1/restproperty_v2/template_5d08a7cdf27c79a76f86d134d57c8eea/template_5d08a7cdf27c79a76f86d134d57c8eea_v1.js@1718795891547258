	$('.arrow-banner').on('click', function(){
		var blockPage = $('.container_style_page'),
			positionBlock = blockPage.offset().top;

		if(blockPage){
			$('body, html').animate({scrollTop: positionBlock}, 600);
		}
	})

	$('.btn-banner').on('click', function(){
		var blockPage = $('.form-block'),
			positionBlock = blockPage.offset().top;

		if(blockPage){
			$('body, html').animate({scrollTop: positionBlock}, 600);
		}
	})



